{
  "$schema": "http://json-schema.org/schema#",
  "$id": "https://github.com/rui-vp-fernandes/SMACovid/blob/main/2ndVersion/Wearable/bloodPressure/schema.jsonld",
  "title": "SMACovid-19 - Blood Pressure schema",
  "description": "Information measured by a given Wearable device",
  "type": "object",
  {
    "properties": {
      "type": {
        "type": "string",
        "enum": ["BloodPressure"],
        "description": "NGSI-LD Entity type"
      },
      "bloodPressureValue": {
        "type": "integer",
        "minimum": 0
      },
      "hasInfoAbout": {
        "type": "string",
        "format": "uri"
      },
    },
  }
  "required": ["type", "id"]
}
